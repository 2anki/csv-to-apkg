{
  "name": "@2anki/csv-to-apkg",
  "version": "1.4.0",
  "description": "convert CSV files to Anki flashcards (APKG)",
  "main": "dist/csv-to-apkg.js",
  "scripts": {
    "build": "npm run prebuild; tsc && vite build",
    "test": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest --coverage",
    "prebuild": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2anki/csv-to-apkg.git"
  },
  "keywords": [
    "notion",
    "title",
    "page",
    "title"
  ],
  "author": "Alexander Alemayhu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2anki/csv-to-apkg/issues"
  },
  "homepage": "https://github.com/2anki/csv-to-apkg#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^9.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^9.0.11",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "semantic-release": "^24.0.0",
    "tslib": "^2.6.0",
    "vitest": "^1.3.1"
  },
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ]
}
